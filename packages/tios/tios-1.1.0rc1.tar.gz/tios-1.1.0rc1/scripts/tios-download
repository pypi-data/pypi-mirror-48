#!/usr/bin/env python
import sys
from tios import transmitter
from tios._version import __version__
import argparse

parser = argparse.ArgumentParser(description='Download copies of the simulation files')

parser.add_argument('id', type=str, help='ID of *tios* simulation')
parser.add_argument('-V', '--version', action='version', version=__version__)
args = parser.parse_args()

te = transmitter.from_entry(args.id)
