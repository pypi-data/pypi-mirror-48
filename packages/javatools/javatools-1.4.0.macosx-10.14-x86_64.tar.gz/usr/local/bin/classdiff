#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'javatools==1.4.0','console_scripts','classdiff'
__requires__ = 'javatools==1.4.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('javatools==1.4.0', 'console_scripts', 'classdiff')()
    )
