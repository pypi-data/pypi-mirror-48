[Unit]
Description=Azure High Availability service
After=waagent.service network.target

[Service]
Type=simple
User=guestshell
Group=guestshell
WorkingDirectory=/home/guestshell/azure/HA
ExecStart=/home/guestshell/.local/lib/python2.7/site-packages/csr_azure_ha/server/ha_server.py start
Restart=always
StandardOutput=null
StandardError=syslog

[Install]
WantedBy=multi-user.target